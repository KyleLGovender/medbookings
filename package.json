{
  "name": "medbookings",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write . --ignore-path .prettierignore --ignore-unknown",
    "fix": "npm run format && npm run lint",
    "prepare": "node -e \"try { if (!process.env.CI && !process.env.AWS_APP_ID) { require('child_process').execSync('husky', {stdio: 'inherit'}) } } catch (e) { }\"",
    "postinstall": "npx prisma generate",
    "seed:production": "npx tsx prisma/seed.production.mts",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:auth": "playwright test e2e/tests/auth/",
    "test:booking": "playwright test e2e/tests/booking/",
    "test:provider": "playwright test e2e/tests/provider/",
    "test:calendar": "playwright test e2e/tests/calendar/",
    "test:legacy": "playwright test e2e/tests/guest-booking-flow.spec.ts e2e/tests/provider-search.spec.ts e2e/tests/error-scenarios.spec.ts e2e/tests/data-cleanup.spec.ts",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:db:setup": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "test:db:seed": "dotenv -e .env.test -- prisma db seed",
    "test:full": "npm run test:db:setup && npm run test:db:migrate && npm run test",
    "test:ci": "npm run test:install && npm run test:full",
    "architecture:check": "git fetch origin && node scripts/architecture/check-core-files.js",
    "compliance:sync": "node scripts/compliance/sync-compliance-rules.js sync",
    "compliance:check": "node scripts/compliance/sync-compliance-rules.js check",
    "compliance:status": "node scripts/compliance/sync-compliance-rules.js status",
    "compliance:setup": "bash scripts/compliance/setup-compliance.sh",
    "compliance:validate": "node scripts/commit-gate/compliance-validator.js validate-file",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev"
  },
